{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "exclude": [
            "node_modules",
            "docs"
        ]
    },
    "opts": {
        "template": "./node_modules/foodoc/template",
        "readme": "README.md",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    },
    "templates": {
        "systemName": "@Marketto/class-mitosis",
        "systemSummary": "Automatic copy and renaming folder/files tool"
    }
}